{
  "name": "web3fuzzforge",
  "version": "1.1.0",
  "description": "CLI tool for generating Web3 test templates",
  "main": "src/index.js",
  "bin": {
    "web3fuzzforge": "src/index.js"
  },
  "dependencies": {
    "@chainsafe/dappeteer": "5.2.1",
    "@playwright/test": "1.41.2",
    "ajv": "8.17.1",
    "chalk": "4.1.2",
    "commander": "11.1.0",
    "dotenv": "16.5.0",
    "ethers": "6.14.1",
    "fs-extra": "11.2.0",
    "inquirer": "8.2.5",
    "openai": "4.24.1",
    "ora": "5.4.1",
    "path": "0.12.7",
    "web3": "4.5.0",
    "Web3FuzzForge": "file:Web3FuzzForge-1.1.0.tgz"
  },
  "devDependencies": {
    "@eslint/js": "8.56.0",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-n": "16.6.2",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.33.2",
    "globals": "14.0.0",
    "prettier": "3.2.5",
    "typescript": "5.3.3"
  },
  "scripts": {
    "start": "node src/index.js",
    "build": "npm pack",
    "prepare": "node -e \"try { process.platform !== 'win32' && require('fs').chmodSync('./src/index.js', '755') } catch (e) { console.log(e) }\"",
    "postinstall": "node -e \"try { process.platform !== 'win32' && require('fs').chmodSync('./src/index.js', '755') } catch (e) { console.log(e) }\"",
    "pretest": "node utils/ensure-reports-dir.js",
    "test": "npx playwright test",
    "posttest": "node utils/merge-reports.js",
    "test:headed": "npx playwright test --headed",
    "test:debug": "npx playwright test --debug",
    "test:report": "npx web3fuzzforge run --report",
    "view:report": "npx playwright show-report reports",
    "merge:reports": "node utils/merge-reports.js",
    "benchmark": "cross-env BENCHMARK_MODE=true npx playwright test --grep-invert @slow --project=chromium --reporter=json,html && node utils/analyze-performance.js",
    "benchmark:compare": "node utils/compare-benchmarks.js --current benchmark-results/results.json --previous benchmark-results/previous.json",
    "benchmark:view": "open benchmark-results/metrics.json",
    "benchmark:ci": "npm run benchmark && node utils/analyze-performance.js --generate-markdown",
    "prelint": "npm run lint:check-config",
    "prelint:fix": "npm run lint:check-config",
    "lint": "eslint .",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "lint:check-config": "node -e \"const fs = require('fs'); if (!fs.existsSync('./.eslintrc.cjs')) { console.error('ERROR: .eslintrc.cjs not found in project root. Please ensure the ESLint config file exists.'); process.exit(1); } else { console.log('.eslintrc.cjs found.'); }\"",
    "lint:ci": "echo 'Skipping linting in CI environment'",
    "format": "prettier --write \"**/*.{js,ts,jsx,tsx,json,md}\"",
    "fix:all": "npm run lint:fix && npm run format",
    "fix:lint:advanced": "node utils/fix-linting.js",
    "web-ui": "node src/web-ui/server.js",
    "tsc": "tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "local-install": "npm install ./Web3FuzzForge-1.1.0.tgz",
    "forge:run": "node ./src/index.js run",
    "forge:gen": "node ./src/index.js generate",
    "security:fix": "node utils/dependency-check.js",
    "audit:fix": "npm audit fix --force || echo 'Manual review required for remaining vulnerabilities'",
    "setup": "bash automation/setup.sh",
    "security:scan": "npm audit --json > security-reports/npm-audit.json && node utils/dependency-check.js --ci > security-reports/dependency-check.log",
    "security:check-dappeteer": "node utils/dependency-check.js",
    "security:run-test:custom": "npx playwright test tests/sample-custom-wallet-connect.test.js --headed",
    "security:run-test:wrapper": "npx playwright test tests/sample-metamask-connect.test.js --headed",
    "security:report": "node -e \"require('./utils/security-report-generator').generateReport()\"",
    "security:full-scan": "npm run security:scan && npm run security:report",
    "test:wallet-interactions": "npx playwright test tests/wallet-interactions/ --headed"
  },
  "engines": {
    "node": ">=16.0.0 <19.0.0",
    "npm": ">=7.0.0"
  },
  "engineStrict": true,
  "resolutions": {
    "minimatch": "^5.1.6",
    "path-to-regexp": "^6.2.1",
    "serve-handler/minimatch": "^5.1.6"
  },
  "keywords": [
    "web3",
    "dapp",
    "testing",
    "security",
    "blockchain",
    "ethereum",
    "metamask"
  ],
  "author": "",
  "license": "MIT",
  "connect_button_selector": "[actual selector from your dApp]",
  "dapp_url": "[actual dApp URL]",
  "wallet": "metamask"
}